<div class="container my-5">
  <div class="card">
    <div class="card-header">
      <h2>Editing Enrollment</h2>
    </div>
    <div class="card-body">
      <%= render "form", enrollment: @enrollment %>

      <div class="mt-3">
        <%= link_to "Show this enrollment", @enrollment, class: 'btn btn-info' %>
        <%= link_to "Back to enrollments", enrollments_path, class: 'btn btn-secondary' %>
      </div>
    </div>
  </div>
</div>